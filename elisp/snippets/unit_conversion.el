;; distance measurements
(defun cm-to-inch (cm) (/ cm 2.54))

(defun inch-to-cm (inch) (* inch 2.54))

(defun cm-to-foot (cm) (/ cm 30.48))

(defun foot-to-cm (foot) (* foot 30.48))

(defun cm-to-yard (cm) (/ cm 91.44))

(defun yard-to-cm (yard) (* yard 91.44))

(defun cm-to-meter (cm) (/ cm 100.0))

(defun meter-to-cm (meter) (* meter 100.0))

(defun meter-to-kilometre (meter) (/ meter 1000.0))

(defun kilometre-to-meter (kilometre) (* kilometre 1000.0))

(defun meter-to-yard (meter) (* meter 1.09361))

(defun yard-to-meter (yard) (/ yard 1.09361))

(defun kilometre-to-mile (kilometre) (/ kilometre 1.609344))

(defun mile-to-kilometre (mile) (* mile 1.609344))

(defun kilometre-to-nautical-mile (kilometre) (/ kilometre 1.852))

(defun nautical-mile-to-kilometre (nautical-mile) (* nautical-mile 1.852))




